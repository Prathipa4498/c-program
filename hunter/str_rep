#include<stdio.h>
int main(void)
{
    char s[20];
    int i,j,l;
    scanf("%s",&s);
    l=strlen(s);
    for(j=0;s[j]!='\0';j++)
    {
        int count=1;
        for(i=j+1;i<l-1;i++)
        {
            if(s[j]==s[i])
            {
                count++;
                s[i]=s[i+1];
                l--;
                s[l]='\0';
                
            }
        }
        printf("%c=%d\n",s[j],count);
    }
}
